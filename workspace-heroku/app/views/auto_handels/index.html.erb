<p id="notice"><%= notice %></p>

<h1>Auto Handels</h1>

<table class="table table-responsive table-hover">
  <thead>
    <tr>
      <th>Marka</th>
      <th>Model</th>
      <th>Cena</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @auto_handels.each do |auto_handel| %>
      <tr>
        <td><%= auto_handel.marka %></td>
        <td><%= auto_handel.model %></td>
        <td><%= auto_handel.cena %></td>
        <td><%= link_to 'Show', auto_handel %></td>
        <% if user_signed_in? %>
        <td><%= link_to 'Edit', edit_auto_handel_path(auto_handel) %></td>
        <td><%= link_to 'Destroy', auto_handel, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        <% else  %>
        <td>Please log in to update and delete <td>
        <% end %>
      </tr>
    <% end %>
  </tbody>
</table>

<center>
  <%= will_paginate @auto_handels %>
  
</center>

<br>

<%= link_to 'New Auto Handel', new_auto_handel_path %>
